        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EBootloaderV3"
        mc:Ignorable="d"
        Title="Reset Device" Height="300" Width="300" Loaded="Window_Loaded">
    <Grid>
        <Label x:Name="lblReset" Content="Please Reset the Device" Margin="10,100,10,0" HorizontalAlignment="Center" VerticalAlignment="Top" Width="272" Height="38" FontSize="18.667">
        </Label>
    </Grid>
</Window>
